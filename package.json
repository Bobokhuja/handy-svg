{
  "name": "handy-svg",
  "version": "1.0.1",
  "description": "A tool for loading svgs to the sprite on the fly",
  "main": "src/index.tsx",
  "scripts": {
    "build": "rm -rf ./dist && tsc --declaration --outDir ./dist",
    "lint": "eslint . && tsc --noEmit",
    "pub": "cp ./package.json ./dist && cp ./README.md ./dist && cd ./dist && npm publish",
    "release": "npm run lint && npm version patch && npm run build && npm run pub",
    "cp": "npm run build && mkdir -p ../geoadv/node_modules/handy-svg && cp -r ./dist/ ../geoadv/node_modules/handy-svg/",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivliag/handy-svg.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivliag/handy-svg/issues"
  },
  "homepage": "https://github.com/ivliag/handy-svg#readme",
  "dependencies": {
    "lodash.debounce": "4.0.8"
  },
  "devDependencies": {
    "@types/lodash.debounce": "4.0.6",
    "@types/react": "17.0.43",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0",
    "husky": "7.0.4",
    "react": "17.0.2",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
